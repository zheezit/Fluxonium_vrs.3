	file = {Osman et al. - 2021 - Simplified Josephson-junction fabrication process .pdf:C\:\\Users\\jiaop\\Zotero\\storage\\DAU9GMTB\\Osman et al. - 2021 - Simplified Josephson-junction fabrication process .pdf:application/pdf},
}

